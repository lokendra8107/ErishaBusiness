
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Email Template</h4>
            <div class="table-responsive pt-3">
                <table id="EmailTemplateTable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>
                                Id
                            </th>
                            <th>
                                Email Title
                            </th>
                            <th>
                                Email Subject
                            </th>
                            <th>
                                Email From
                            </th>
                            <th>
                                Active
                            </th>
                            <th>
                                Action
                            </th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/admin/pagesscript/emailtemplate/index.js"></script>